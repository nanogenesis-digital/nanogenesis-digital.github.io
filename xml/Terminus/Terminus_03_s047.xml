               <title n="" xml:id="T-Terminus-047_d1e50" prev="#" next="#"><add seq="20130925085236" type="context" evidence="499-506">TERMINUS</add>
                  <del seq="20130925085238" type="context" evidence="507-523">EPILOOG</del></title>
            </p>
            <p>
               <seg n="Terminus-047,[4108]" type="metamark" xml:id="Terminus-047_d1e56" prev="#" next="#Terminus-047_d1e59">BEGIN: Tss.</seg> 
               <seg n="Terminus-047,[Fr10|002]" type="metamark" xml:id="Terminus-047_d1e59" prev="#Terminus-047_d1e56" next="#Terminus-047_d1e65">(cf. openingswoord: Psst)</seg>
            <lb/>
               <seg n="Terminus-047,[4109]" xml:id="Terminus-047_d1e65" prev="#Terminus-047_d1e59" next="#Terminus-047_d1e68">Wat een gedoe, h√®.</seg> 
               <seg n="Terminus-047,[4110]" xml:id="Terminus-047_d1e68" prev="#Terminus-047_d1e65" next="#Terminus-047_d1e74">Tjongejonge.</seg>
            </p>
            <p>
               <seg n="Terminus-047,[4113]" xml:id="Terminus-047_d1e74" prev="#Terminus-047_d1e68" next="#Terminus-047_d1e77">Zie je bloed?</seg> 
               <seg n="Terminus-047,[Fr10|003]" type="metamark" xml:id="Terminus-047_d1e77" prev="#Terminus-047_d1e74" next="#Terminus-047_d1e83">/ (En overal ruikt het naar bloed ofschoon nergens bloed te bespeuren valt.)</seg>
            </p>
            <p>
               <seg n="Terminus-047,[Fr10|004]" type="metamark" xml:id="Terminus-047_d1e83" prev="#Terminus-047_d1e77" next="#Terminus-047_d1e90">Dan:  Al het voorgaande wordt op de helling gezet.</seg>
            </p>
            <p>
               <seg n="Terminus-047,[Fr10|005]" type="metamark" xml:id="Terminus-047_d1e90" prev="#Terminus-047_d1e83" next="#Terminus-047_d1e93">Alle personages leven nog.</seg> 
               <seg n="Terminus-047,[Fr10|006]" type="metamark" xml:id="Terminus-047_d1e93" prev="#Terminus-047_d1e90" next="#Terminus-047_d1e96">Doen andere dingen.</seg> 
               <seg n="Terminus-047,[Fr10|007]" type="metamark" xml:id="Terminus-047_d1e96" prev="#Terminus-047_d1e93" next="#Terminus-047_d1e99">Van alle verhalen klopt niets.</seg> 
               <seg n="Terminus-047,[4130]" type="metamark" xml:id="Terminus-047_d1e99" prev="#Terminus-047_d1e96" next="#Terminus-047_d1e102">Barry loopt fluitend weg.</seg> 
               <seg n="Terminus-047,[4122]" type="metamark" xml:id="Terminus-047_d1e102" prev="#Terminus-047_d1e99" next="#Terminus-047_d1e106">Gaard leeft nog/weer.</seg> 
               <seg n="Terminus-047,[Fr10|008]" type="metamark" xml:id="Terminus-047_d1e106" prev="#Terminus-047_d1e102" next="#Terminus-047_d1e109">Helge, Janine en Jella lopen verder.</seg> 
               <seg n="Terminus-047,[Fr10|009]" type="metamark" xml:id="Terminus-047_d1e109" prev="#Terminus-047_d1e106" next="#Terminus-047_d1e115">Carla loopt naar haar fiets terwijl R tegen haar praat.</seg>
            <lb type="indent"/>
               <seg n="Terminus-047,[4127]" type="metamark" xml:id="Terminus-047_d1e115" prev="#Terminus-047_d1e109" next="#Terminus-047_d1e121">En X kijkt naar de stoeptegels voor hem/haar alsof er iets op geschreven staat.</seg>
               <lb type="indent"/>
               <seg n="Terminus-047,[4148]" xml:id="Terminus-047_d1e121" prev="#Terminus-047_d1e115" next="#Terminus-047_d1e127">Ik zei toch al dat je me niet moest geloven?</seg>
               <lb type="indent"/>
               <seg n="Terminus-047,[4148|001]" xml:id="Terminus-047_d1e127" prev="#Terminus-047_d1e121" next="#Terminus-047_d1e133">Pleinen zijn (volstrekt) onbetrouwbaar.</seg>
               <lb type="indent"/>
               <seg n="Terminus-047,[4149]" xml:id="Terminus-047_d1e133" prev="#Terminus-047_d1e127" next="#">Geloof ze nooit.</seg>
            </p>

