         <said who="Franklin-10">
            <p>
               <title n="" xml:id="Terminus-048_d1e50" prev="#" next="#Terminus-048_d1e56">TERMINUS</title>
            </p>
            <p>
               <seg n="Terminus-048,[4108]" type="metamark" xml:id="Terminus-048_d1e56" prev="#Terminus-048_d1e50" next="#Terminus-048_d1e59">BEGIN: Tss.</seg> 
               <seg n="Terminus-048,[Fr10|002]" type="metamark" xml:id="Terminus-048_d1e59" prev="#Terminus-048_d1e56" next="#Terminus-048_d1e65">(cf. openingswoord: Psst)</seg>
               <lb/>
               <seg n="Terminus-048,[4109]" xml:id="Terminus-048_d1e65" prev="#Terminus-048_d1e59" next="#Terminus-048_d1e68">Wat een gedoe, h√®.</seg> 
               <seg n="Terminus-048,[4110]" xml:id="Terminus-048_d1e68" prev="#Terminus-048_d1e65" next="#Terminus-048_d1e74">Tjongejonge.</seg>
            </p>
            <p>
               <seg n="Terminus-048,[4113]" xml:id="Terminus-048_d1e74" prev="#Terminus-048_d1e68" next="#Terminus-048_d1e77">Zie je bloed?</seg> 
               <seg n="Terminus-048,[Fr10|003]" type="metamark" xml:id="Terminus-048_d1e77" prev="#Terminus-048_d1e74" next="#Terminus-048_d1e83">/ (En overal ruikt het naar bloed ofschoon nergens bloed te bespeuren valt.)</seg>
            </p>
            <p>
               <seg n="Terminus-048,[Fr10|004]" type="metamark" xml:id="Terminus-048_d1e83" prev="#Terminus-048_d1e77" next="#Terminus-048_d1e90">Dan:  Al het voorgaande wordt op de helling gezet.</seg>
            </p>
            <p>
               <seg n="Terminus-048,[Fr10|005]" type="metamark" xml:id="Terminus-048_d1e90" prev="#Terminus-048_d1e83" next="#Terminus-048_d1e93">Alle personages leven nog.</seg> 
               <seg n="Terminus-048,[Fr10|006]" type="metamark" xml:id="Terminus-048_d1e93" prev="#Terminus-048_d1e90" next="#Terminus-048_d1e96">Doen andere dingen.</seg> 
               <seg n="Terminus-048,[Fr10|007]" type="metamark" xml:id="Terminus-048_d1e96" prev="#Terminus-048_d1e93" next="#Terminus-048_d1e99">Van alle verhalen klopt niets.</seg> 
               <seg n="Terminus-048,[4130]" type="metamark" xml:id="Terminus-048_d1e99" prev="#Terminus-048_d1e96" next="#Terminus-048_d1e102">Barry loopt fluitend weg.</seg> 
               <seg n="Terminus-048,[4122]" type="metamark" xml:id="Terminus-048_d1e102" prev="#Terminus-048_d1e99" next="#Terminus-048_d1e106">Gaard leeft nog/weer.</seg> 
               <seg n="Terminus-048,[Fr10|008]" type="metamark" xml:id="Terminus-048_d1e106" prev="#Terminus-048_d1e102" next="#Terminus-048_d1e112">Helge, Janine en Jella lopen verder.</seg>
               <del seq="20130925154757" xml:id="Terminus-048_translocation1" type="translocation" evidence="7119-7123"><seg n="Terminus-048,[4136]" xml:id="Terminus-048d_d1e118"><add seq="20130925154630" type="nt" evidence="6610-6617">En jij?</add></seg> 
               <seg n="Terminus-048,[Fr10|009]" xml:id="Terminus-048d_d1e109"><del seq="20130925154641" type="context" evidence="6672-6835">Carla loopt naar haar fiets terwijl R tegen haar praat.</del></seg>
                  <seg n="Terminus-048,[4144]" xml:id="Terminus-048d_d1e121"><add seq="20130925154648" type="nt" evidence="6839-6867">Je werk(dag)(week) zit er op.</add></seg> 
                  <seg n="Terminus-048,[4145]" xml:id="Terminus-048d_d1e124"><add seq="20130925154659" type="nt" evidence="6870-6889">Je hebt goed geteld.</add></seg> 
                  <seg n="Terminus-048,[4142]" xml:id="Terminus-048d_d1e127"><add seq="20130925154708" type="nt" evidence="6892-7059">Haal <add seq="20130925154708" type="typo" evidence="7056" instant="false">j</add>e fiets maar<seg n="Terminus-048,[4143]"> en rij naar huis<mod seq="20130925154724" type="continue" evidence="7059">.</mod></seg></add></seg> 
                  <seg n="Terminus-048,[4146]" xml:id="Terminus-048d_d1e130"><add seq="20130925154734" type="nt" evidence="7068-7102">Het nummer is twee<del seq="20130925154740" type="typo" evidence="7088-7089" instant="true">; </del></add><add seq="20130925154741" type="nt|continue" evidence="7090-7102">, zes, zeven.</add></seg> 
                  <seg n="Terminus-048,[4147]" xml:id="Terminus-048d_d1e134"><add seq="20130925154747" type="nt" evidence="7105-7115">Niet negen.</add></seg></del>
               <lb type="indent"/>
               <seg n="Terminus-048,[4127]" type="metamark" xml:id="Terminus-048_d1e112" prev="#Terminus-048_d1e106" next="#Terminus-048_d1e118">En X kijkt naar de stoeptegels voor hem/haar alsof er iets op geschreven staat.</seg>
               <lb type="indent"/>
               <add seq="20130925154808" type="translocation" evidence="7140" sameAs="#Terminus-048_translocation1"><seg n="Terminus-048,[4136]" xml:id="Terminus-048_d1e118" prev="#Terminus-048_d1e112" next="#Terminus-048_d1e121">En jij?</seg> 
                  <seg n="Terminus-048,[4144]" xml:id="Terminus-048_d1e121" prev="#Terminus-048_d1e118" next="#Terminus-048_d1e124">Je werk(dag)(week) zit er op.</seg> 
                  <seg n="Terminus-048,[4145]" xml:id="Terminus-048_d1e124" prev="#Terminus-048_d1e121" next="#Terminus-048_d1e127">Je hebt goed geteld.</seg> 
                  <seg n="Terminus-048,[4142]" xml:id="Terminus-048_d1e127" prev="#Terminus-048_d1e124" next="#Terminus-048_d1e130">Haal je fiets maar<seg n="Terminus-048,[4143]"> en rij naar huis.</seg></seg> 
                  <seg n="Terminus-048,[4146]" xml:id="Terminus-048_d1e130" prev="#Terminus-048_d1e127" next="#Terminus-048_d1e134">Het nummer is twee, zes, zeven.</seg> 
                  <seg n="Terminus-048,[4147]" xml:id="Terminus-048_d1e134" prev="#Terminus-048_d1e130" next="#Terminus-048_d1e140">Niet negen.</seg></add>
               <lb type="indent"/>
               <seg n="Terminus-048,[4148]" xml:id="Terminus-048_d1e140" prev="#Terminus-048_d1e134" next="#Terminus-048_d1e146">Ik zei toch al dat je me niet moest geloven?</seg>
               <lb type="indent"/>
               <seg n="Terminus-048,[4148|001]" xml:id="Terminus-048_d1e146" prev="#Terminus-048_d1e140" next="#Terminus-048_d1e149">Pleinen zijn (volstrekt) onbetrouwbaar.</seg> 
               <seg n="Terminus-048,[4149]" xml:id="Terminus-048_d1e149" prev="#Terminus-048_d1e146" next="#">Geloof ze nooit.</seg>
            </p>
         </said>
