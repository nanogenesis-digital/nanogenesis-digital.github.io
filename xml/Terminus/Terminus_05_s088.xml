            <p>
               <title n="" xml:id="T-Terminus-088_d1e50" prev="#" next="#">TERMINUS</title>
            </p>
            <p>
               <seg n="Terminus-088,[4108]" type="metamark" xml:id="Terminus-088_d1e56" prev="#" next="#Terminus-088_d1e59">BEGIN: Tss.</seg> 
               <seg n="Terminus-088,[Fr10|002]" type="metamark" xml:id="Terminus-088_d1e59" prev="#Terminus-088_d1e56" next="#Terminus-088_d1e65">(cf. openingswoord: Psst)</seg>
            <lb/>
               <seg n="Terminus-088,[4109]" xml:id="Terminus-088_d1e65" prev="#Terminus-088_d1e59" next="#Terminus-088_d1e68">Wat een gedoe, h√®.</seg> 
               <seg n="Terminus-088,[4110]" xml:id="Terminus-088_d1e68" prev="#Terminus-088_d1e65" next="#Terminus-088_d1e74">Tjongejonge.</seg>
            </p>
            <p>
               <seg n="Terminus-088,[4113]" xml:id="Terminus-088_d1e74" prev="#Terminus-088_d1e68" next="#Terminus-088_d1e77">Zie je bloed?</seg> 
               <seg n="Terminus-088,[Fr10|003]" type="metamark" xml:id="Terminus-088_d1e77" prev="#Terminus-088_d1e74" next="#Terminus-088_d1e83">/ (En overal ruikt het naar bloed ofschoon nergens bloed te bespeuren valt.)</seg>
            </p>
            <p>
               <seg n="Terminus-088,[Fr10|004]" type="metamark" xml:id="Terminus-088_d1e83" prev="#Terminus-088_d1e77" next="#Terminus-088_d1e90">Dan:  Al het voorgaande wordt op de helling gezet.</seg>
            </p>
            <p>
               <seg n="Terminus-088,[Fr10|005]" type="metamark" xml:id="Terminus-088_d1e90" prev="#Terminus-088_d1e83" next="#Terminus-088_d1e93">Alle personages leven nog.</seg> 
               <seg n="Terminus-088,[Fr10|006]" type="metamark" xml:id="Terminus-088_d1e93" prev="#Terminus-088_d1e90" next="#Terminus-088_d1e96">Doen andere dingen.</seg> 
               <seg n="Terminus-088,[Fr10|007]" type="metamark" xml:id="Terminus-088_d1e96" prev="#Terminus-088_d1e93" next="#Terminus-088_d1e99">Van alle verhalen klopt niets.</seg> 
               <seg n="Terminus-088,[4130]" type="metamark" xml:id="Terminus-088_d1e99" prev="#Terminus-088_d1e96" next="#Terminus-088_d1e102">Barry loopt fluitend weg.</seg> 
               <seg n="Terminus-088,[4122]" type="metamark" xml:id="Terminus-088_d1e102" prev="#Terminus-088_d1e99" next="#Terminus-088_d1e106">Gaard leeft nog/weer.</seg> 
               <seg n="Terminus-088,[Fr10|008]" type="metamark" xml:id="Terminus-088_d1e106" prev="#Terminus-088_d1e102" next="#Terminus-088_d1e112">Helge, Janine en Jella lopen verder.</seg>
               <lb type="indent"/>
               <seg n="Terminus-088,[4127]" type="metamark" xml:id="Terminus-088_d1e112" prev="#Terminus-088_d1e106" next="#Terminus-088_d1e118">En X kijkt naar de stoeptegels voor hem/haar alsof er iets op geschreven staat.</seg>
               <lb type="indent"/>
               <seg n="Terminus-088,[4136]" xml:id="Terminus-088_d1e118" prev="#Terminus-088_d1e112" next="#Terminus-088_d1e121">En jij?</seg> 
               <seg n="Terminus-088,[4144]" xml:id="Terminus-088_d1e121" prev="#Terminus-088_d1e118" next="#Terminus-088_d1e124">Je werk(dag)(week) zit er op.</seg> 
               <seg n="Terminus-088,[4145]" xml:id="Terminus-088_d1e124" prev="#Terminus-088_d1e121" next="#Terminus-088_d1e127">Je hebt goed geteld.</seg> 
               <seg n="Terminus-088,[4142]" xml:id="Terminus-088_d1e127" prev="#Terminus-088_d1e124" next="#Terminus-088_d1e130">Haal je fiets maar<seg n="Terminus-088,[4143]"> en rij naar huis.</seg></seg> 
               <seg n="Terminus-088,[4146]" xml:id="Terminus-088_d1e130" prev="#Terminus-088_d1e127" next="#Terminus-088_d1e134">Het nummer is twee, zes, zeven.</seg> 
               <seg n="Terminus-088,[4147]" xml:id="Terminus-088_d1e134" prev="#Terminus-088_d1e130" next="#Terminus-088_d1e140">Niet negen.</seg>
               <lb type="indent"/>
               <seg n="Terminus-088,[4148]" xml:id="Terminus-088_d1e140" prev="#Terminus-088_d1e134" next="#Terminus-088_d1e146">Ik zei toch al dat je me niet moest geloven?</seg>
               <lb type="indent"/>
               <seg n="Terminus-088,[4148|001]" xml:id="Terminus-088_d1e146" prev="#Terminus-088_d1e140" next="#Terminus-088_d1e149">Pleinen zijn (volstrekt) onbetrouwbaar.</seg> 
               <seg n="Terminus-088,[4149]" xml:id="Terminus-088_d1e149" prev="#Terminus-088_d1e146" next="#Terminus-088_d1e155">Geloof ze nooit.</seg>
            </p>
            <p>
               <seg n="Terminus-088,[4149|001]" type="metamark" xml:id="Terminus-088_d1e155" prev="#Terminus-088_d1e149" next="#Terminus-088_d1e162"><add seq="20131202173328" type="nt" evidence="13078-13186">OF: 	Pl<del seq="20131202173337" type="typo" evidence="13091-13096" instant="true">zinzn </del></add><add seq="20131202173338" type="nt|continue" evidence="13097-13186">ei<del seq="20131202173341" type="typo" evidence="13114-13115" instant="false">e</del>nen<mod seq="20131202173342" type="continue" evidence="13123-13139"> zijn vol<del seq="20131202173347" type="typo" evidence="13156-13157" instant="false">q</del>sterkt </mod><mod seq="20131202173349" type="continue" evidence="13173-13186">onbetrouwbaar.</mod></add></seg>
               <lb type="indent"/>
               <seg n="Terminus-088,[4149|002]" xml:id="Terminus-088_d1e162" prev="#Terminus-088_d1e155" next="#"><add seq="20131202173405" type="nt" evidence="13197-13244"><del seq="20131202173405" type="pre-context" evidence="13198">G</del></add><add seq="20131202173406" type="nt|continue" evidence="13199-13244">Ik zei toch al dat je me niet moest geloven?</add></seg>
            </p>
   
